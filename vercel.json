{
  "rewrites": [
    { "source": "/npm/(.*)", "destination": "/npm" },
    { "source": "/crates/(.*)", "destination": "/crates" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, s-maxage=300, stale-while-revalidate=86400" }
      ]
    }
  ]
}
